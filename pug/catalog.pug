extends includes/head.pug
block body

	body
		include includes/header.pug

		main

			section.breadcrumbs
				.wrapper
					ul
						li
							a(href="index.html") Главная
						li
							span Каталог автомашин

			section.top
				.wrapper
					.top__text
						h1 Каталог автомашин 
						p Повседневная практика показывает, что дальнейшее развитие различных форм деятельности влечет за собой процесс внедрения и модернизации модели развития.

			section.filter
				.wrapper
					.filter__type
						ul
							li.li_active
								a(href="#").btn Все
							li
								a(href="#").btn Mercedes Benz
							li
								a(href="#").btn BMW
							li
								a(href="#").btn Audi
							li
								a(href="#").btn Porsche
							li
								a(href="#").btn Range Rover
					.filter__type_mob
						ul.type__all
							li
								a(href="#").btn Все
						.select
							.select__head
								span.btn Mercedes Benz
							ul.select__body
								li
									a(href="#").select__item.btn Mercedes Benz
								li
									a(href="#").select__item.btn BMW
								li
									a(href="#").select__item.btn Audi
								li
									a(href="#").select__item.btn Porsche
								li
									a(href="#").select__item.btn Range Rover


					.filter__sort
						p Выводить по:
						.select
							.select__head
								span 10
							.select__body
								.select__item 20
								.select__item 30

