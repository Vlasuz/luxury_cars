extends includes/head.pug
block body

	body
		include includes/header.pug

		main

			section.breadcrumbs
				.wrapper
					ul
						li
							a(href="index.html") Главная
						li
							span Каталог автомашин

			section.top.catalog__top
				.wrapper
					.top__text
						h1 Каталог автомашин 
						ul
							li Гарантируем подлинность пробега
							li Прозрачное ценообразование
							li Растаможивание для ЕАЭС

			section.filter
				.wrapper
					.filter__type
						ul
							li.li_active(data-filter="all")
								a(href="#").btn Все
							li(data-filter="audi")
								a(href="#").btn
									img(src="img/cars-marks/audi.svg" alt="")
							li(data-filter="bmw")
								a(href="#").btn
									img(src="img/cars-marks/bmw-logo.svg" alt="")
							li(data-filter="mercedes")
								a(href="#").btn
									img(src="img/cars-marks/mers.svg" alt="")
							li(data-filter="porsche")
								a(href="#").btn
									img(src="img/cars-marks/porsche.svg" alt="")
							li(data-filter="range_rover")
								a(href="#").btn
									img(src="img/cars-marks/range-rover.svg" alt="")
					.filter__type_mob
						ul.type__all
							li
								a(href="#").btn Все
						.select
							.select__head
								span.btn Mercedes Benz
							ul.select__body
								li(data-filter="audi")
									a(href="#").btn
										img(src="img/cars-marks/audi.svg" alt="")
								li(data-filter="bmw")
									a(href="#").btn
										img(src="img/cars-marks/bmw-logo.svg" alt="")
								li(data-filter="mercedes")
									a(href="#").btn
										img(src="img/cars-marks/mers.svg" alt="")
								li(data-filter="porsche")
									a(href="#").btn
										img(src="img/cars-marks/porsche.svg" alt="")
								li(data-filter="range_rover")
									a(href="#").btn
										img(src="img/cars-marks/range-rover.svg" alt="")


					//- .filter__sort
						p Выводить по:
						.select
							.select__head
								span 10
							.select__body
								.select__item 20
								.select__item 30

			section.catalog
				.wrapper
					.catalog__inner
						.catalog__mark(data-mark="bmw")
							h3 BMW 
							.catalog__grid
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
						.catalog__mark(data-mark="mercedes")
							h3 Mercedes-Benz
							.catalog__grid
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
						.catalog__mark(data-mark="audi")
							h3 Audi
							.catalog__grid
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
								+catalog_item('BMW 77')
